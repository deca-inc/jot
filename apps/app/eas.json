{
  "cli": {
    "version": ">= 16.27.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "node": "22.12.0",
      "pnpm": "8.8.0",
      "ios": {
        "image": "macos-sequoia-15.4-xcode-16.3",
        "prebuildCommand": "brew install cmake"
      },
      "env": {
        "EXPO_PUBLIC_APP_VARIANT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "node": "22.12.0",
      "pnpm": "8.8.0",
      "ios": {
        "image": "macos-sequoia-15.4-xcode-16.3",
        "prebuildCommand": "brew install cmake"
      },
      "env": {
        "EXPO_PUBLIC_APP_VARIANT": "development"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "node": "22.12.0",
      "pnpm": "8.8.0",
      "ios": {
        "image": "macos-sequoia-15.4-xcode-16.3",
        "prebuildCommand": "brew install cmake"
      },
      "env": {
        "EXPO_PUBLIC_APP_VARIANT": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
